{
    "name": "testcenter-web",
    "version": "2.69.0",
    "author": "CGI",
    "description": "Testcenter Web",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "https://bitbucket.wada-ama.org/scm/adams-ng/testcenter-web.git"
    },
    "scripts": {
        "ng": "ng",
        "start": "ng serve --ssl --live-reload=false --host 0.0.0.0 --public-host https://wada.wsl.host",
        "test": "ng test",
        "lint": "npx eslint . --ext ts",
        "lint:fix": "npx eslint . --ext ts --fix",
        "e2e": "ng e2e",
        "compodoc": "npx compodoc -p src/tsconfig.app.json",
        "i18n:extract": "ng extract-i18n",
        "build": "ng build --configuration $environment && npm run compress:bundle",
        "compress:bundle": "gzipper compress ./dist --verbose",
        "version:patch": "npm --no-git-tag-version version patch",
        "folder": "rm -rf server/testcenter-webserver && cd server && mkdir testcenter-webserver && cd ..",
        "bundle": "npm run folder  && cp -r ./dist ./server/testcenter-webserver/public && cp -r ./server/ssl ./server/testcenter-webserver",
        "zip": "rm -rf server/testcenter-webserver.zip && cd server/testcenter-webserver && zip -rq ../testcenter-webserver.zip *",
        "local:docker": "sh ./build-deploy.sh testing && sh ./build-local-docker.sh"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint && npm run i18n:extract && git add **messages.xlf"
        }
    },
    "dependencies": {
        "@angular/animations": "^11.2.3",
        "@angular/cdk": "^11.2.2",
        "@angular/common": "^11.2.3",
        "@angular/compiler": "^11.2.3",
        "@angular/core": "^11.2.3",
        "@angular/forms": "^11.2.3",
        "@angular/localize": "^11.2.3",
        "@angular/material": "^11.2.2",
        "@angular/platform-browser": "^11.2.3",
        "@angular/platform-browser-dynamic": "^11.2.3",
        "@angular/router": "^11.2.3",
        "@elastic/apm-rum-angular": "^2.1.3",
        "@ngrx/effects": "^11.0.1",
        "@ngrx/router-store": "^11.0.1",
        "@ngrx/store": "^11.0.1",
        "@ngrx/store-devtools": "^11.0.1",
        "@types/google-libphonenumber": "^7.4.17",
        "@types/lodash": "^4.14.168",
        "@types/lodash-es": "^4.17.4",
        "@wada/node-build-manifest-manager": "^1.0.0",
        "angular-resize-event": "^2.1.0",
        "backoff-rxjs": "^6.5.7",
        "bootstrap": "^4.3.1",
        "classlist.js": "^1.1.20150312",
        "core-js": "^2.4.1",
        "flatpickr": "^4.6.9",
        "google-libphonenumber": "^3.1.15",
        "lodash-es": "^4.17.21",
        "moment": "^2.22.2",
        "moment-timezone": "^0.5.43",
        "ng2-date-picker": "^11.0.0",
        "ng2-flatpickr": "^9.0.0",
        "ngrx-forms": "^6.3.4",
        "ngx-bootstrap": "^7.1.0",
        "ngx-cookie-service": "^11.0.2",
        "ngx-i18nsupport": "^0.17.0",
        "ngx-toastr": "^13.2.0",
        "rxjs": "^6.6.6",
        "tslib": "^2.0.0",
        "uuid": "^8.3.2",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.1102.2",
        "@angular/cli": "^11.2.2",
        "@angular/compiler-cli": "^11.2.3",
        "@angular/language-service": "^11.2.3",
        "@compodoc/compodoc": "^1.1.11",
        "@types/jasmine": "~3.6.0",
        "@types/jasminewd2": "~2.0.2",
        "@types/node": "^12.11.1",
        "@types/uuid": "^8.3.0",
        "@typescript-eslint/eslint-plugin": "^4.19.0",
        "@typescript-eslint/parser": "^4.19.0",
        "concurrently": "^5.3.0",
        "eslint": "^7.22.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-prettier": "^8.1.0",
